    ] ;
  skos:example """
ex:SM4
    rdf:type proj:SumOfMoney ;
    proj:moneyAmount \"524000000\"^^xsd:decimal ;
    proj:moneyCurrency <https://dbpedia.org/resource/Australian_dollar> ;
 .
""" ;
